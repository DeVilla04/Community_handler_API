{
  "name": "community_handler_api",
  "version": "1.0.0",
  "description": "Clean code converted from ts-version",
  "main": "index.js",
  "repository": "https://github.com/DeVilla04/Community_handler_API/tree/clean-code",
  "author": "Vraj Dalal/ DeVilla04",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production env-cmd -f config/v1/.env.production ts-node-dev -r tsconfig-paths/register --transpile-only index.ts",
    "dev": "NODE_ENV=development env-cmd -f config/v1/.env.development ts-node-dev -r tsconfig-paths/register --transpile-only index.ts",
    "start-env": "NODE_ENV=$BUILD_ENV env-cmd -f config/v1/.env.$BUILD_ENV ts-node-dev -r tsconfig-paths/register --transpile-only index.ts",
    "prisma-migrate": "env-cmd -f config/v1/.env.development npx prisma migrate dev --name init",
    "prisma-generate": "env-cmd -f config/v1/.env.development prisma generate",
    "prisma-seed": "env-cmd -f config/v1/.env.development prisma db seed",
    "prisma-reset": "env-cmd -f config/v1/.env.development prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^4.14.1",
    "@theinternetfolks/snowflake": "^1.3.0",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.3.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.1",
    "sequelize-typescript": "^2.1.5",
    "tsconfig-paths": "^4.2.0",
    "validatorjs": "^3.22.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.1",
    "@types/validator": "^13.7.17",
    "@types/validatorjs": "^3.15.0",
    "nodemon": "^2.0.22",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
